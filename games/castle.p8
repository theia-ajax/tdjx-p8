function make_platform(x,y,w,‚ñë)
	local e=make_entity({
			name="platform",
			x=x,y=y,
			dx=1,dy=0,
			nextdx=nil,
			wait_‚ßó=0,
			ix=1,iy=1,
			mdx=0,mdy=0,
			g=0,
			w=w or 4,
			h=2,
			sh=4,
			‚ñë=‚ñë,
			collide=true,
			on_update=platform_update,
			on_hit_wall=function(e,side)
				if e.nextdx==nil then
					e.wait_‚ßó=22
					if side>0 then
						e.nextdx=-abs(e.ldx)
					else
						e.nextdx=abs(e.ldx)
					end
				end
			end,
			on_draw=platform_draw,
		})
	tblcpy(opt or {},e)
	make_entity_solid(e)
	pf=e
	return e

function platform_update(e)
	if e.wait_‚ßó>0 then
		e.wait_‚ßó-=1
	else
		if e.nextdx then
			e.dx=e.nextdx
			e.nextdx=nil
		end
	end
	e.mdx=e.dx
	entity_update(e)
end


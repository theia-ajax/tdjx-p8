function _init()
	actors={}
	cam={x=0,y=0}
	world_init(0,0,32,16)
end

function _update()
	local ix,iy=0,0
	if (btn(0)) ix-=1
	if (btn(1)) ix+=1
	if (btn(2)) iy-=1
	if (btn(3)) iy+=1

	player.input={
		x=ix,y=iy,j=btnp(4)
	}
	
	if btnp(5) then
		add(actors,article:new({
			kind=40,x=player.x,y=player.y-0.5,
			dx=player.face*0.4,dy=-0.1
		}))
	end

	for actor in all(actors) do
		actor:move()
	end
	
	cam.x=(player.x-8)*8
end

function _draw()
	cls()
	
	camera(cam.x,cam.y)
	map(world.x,world.y,0,0,world.w,world.h)
	
	for actor in all(actors) do
		actor:draw()
	end

	camera()
	
	dbg_draw()
end

function inherits(base)
 local new={}

 function new:new(p)
 	self.__index=self
  return
  	setmetatable(p or {},self)
 end

 if base then
  setmetatable(new,{
  	__index=base
  })
 end

 return new
end
